// Background
material Background
{
    receive_shadows off
    technique
    {
        pass
        {
            depth_write off
            ambient 0.0 0.0 0.0 1.0
            diffuse 0.8 0.8 0.8 1.0
            specular 0.0 0.0 0.0 1.0 2.0
            emissive 1.0 1.0 1.0 1.0
            texture_unit
            {
                texture starsedit.jpg
                tex_address_mode wrap
                scale 1.0 1.0
                filtering trilinear
            }
        }
    }
}
